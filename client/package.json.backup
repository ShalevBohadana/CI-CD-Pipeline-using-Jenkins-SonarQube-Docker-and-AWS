{
  "name": "fullboosts",
  "version": "0.4.0",
  "private": true,
  "packageManager": "pnpm@8.7.6",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "add-commit-push": "git add . && pnpm run commitizen && git push",
    "check:type": "tsc -p . --noEmit",
    "check:format": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,scss,html,md,json}\"",
    "check:lint": "eslint src --ext .ts,.tsx,.js,.jsx --max-warnings=0",
    "fix:format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,html,md,json}\"",
    "fix:lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "fix:all": "pnpm run fix:format && pnpm run fix:lint && pnpm run check:type",
    "pretest": "pnpm run fix:lint",
    "test": "jest --colors --passWithNoTests",
    "posttest": "pnpm dlx http-server coverage/lcov-report",
    "test:watch": "pnpm run test --collectCoverage=false --watch",
    "lint-staged": "lint-staged",
    "commitizen": "cz",
    "release": "dotenv release-it"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "infile": "CHANGELOG.md",
        "preset": {
          "name": "conventionalcommits",
          "types": [
            {
              "type": "feat",
              "section": "Features"
            },
            {
              "type": "fix",
              "section": "Bug Fixes"
            },
            {}
          ]
        }
      }
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@floating-ui/react": "^0.26.6",
    "@headlessui/react": "1.7.17",
    "@hookform/resolvers": "^3.3.4",
    "@loadable/component": "^5.16.3",
    "@radix-ui/react-slot": "^1.1.1",
    "@reduxjs/toolkit": "^2.0.1",
    "@stripe/react-stripe-js": "^2.4.0",
    "@stripe/stripe-js": "^2.3.0",
    "@tawk.to/tawk-messenger-react": "^2.0.2",
    "axios": "^1.6.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "dompurify": "^3.0.8",
    "firebase": "^10.7.2",
    "framer-motion": "^10.18.0",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.469.0",
    "moment": "^2.30.1",
    "primereact": "^10.3.3",
    "react": "^18.2.0",
    "react-datepicker": "^4.25.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.49.3",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.0.1",
    "react-images-uploading": "^3.1.7",
    "react-lazily": "^0.9.2",
    "react-password-strength-bar": "^0.4.1",
    "react-quill": "^2.0.0",
    "react-redux": "^9.1.0",
    "react-responsive-modal": "^6.4.2",
    "react-router-dom": "^6.21.3",
    "react-select": "^5.8.0",
    "react-tabs": "^6.0.2",
    "react-tag-autocomplete": "^7.1.0",
    "reactjs-popup": "^2.0.6",
    "socket.io-client": "^4.7.4",
    "swiper": "^11.0.5",
    "tailwind-merge": "^2.5.5",
    "uuid": "^9.0.1",
    "yup": "^1.6.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@release-it/conventional-changelog": "^8.0.1",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@types/crypto-js": "^4.2.2",
    "@types/dompurify": "^3.2.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.11",
    "@types/js-cookie": "^3.0.6",
    "@types/loadable__component": "^5.13.8",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.17.17",
    "@types/react": "^18.2.48",
    "@types/react-datepicker": "^4.19.5",
    "@types/react-dom": "^18.2.18",
    "@types/react-helmet": "^6.1.11",
    "@types/react-redux": "^7.1.33",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "discord.js": "14.13.0",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "glob": "^11.0.1",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.4.3",
    "postcss": "^8.4.33",
    "postcss-load-config": "^5.0.2",
    "prettier": "^3.2.4",
    "release-it": "^17.0.1",
    "rollup": "^4.9.5",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vite": "^5.0.12",
    "vite-plugin-mkcert": "^1.17.3"
  }
}
