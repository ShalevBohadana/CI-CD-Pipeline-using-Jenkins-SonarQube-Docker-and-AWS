import { createSlice } from '@reduxjs/toolkit';

type Wallet = {
  balance: number;
};

const initialState: Wallet = {
  balance: 0,
};

export const walletSlice = createSlice({
  name: 'wallet',
  initialState,
  reducers: {},
});

// export const {} = walletSlice.actions;
export const walletReducer = walletSlice.reducer;
